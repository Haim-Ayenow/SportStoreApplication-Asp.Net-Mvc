@using SportStoreApplication.Models;


<table style=" border: black 1px solid; ">
    <tr>
        <th>Id</th>
        <th>Category</th>
        <th>Product Name</th>
        <th>Company</th>
        <th>Price</th>
        <th>Amount</th>
        <th>Team Id</th>
        <th>Picture</th>
    </tr>
    @foreach (SportsEquipment item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Category</td>
            <td>@item.ProductName</td>
            <td>@item.Company</td>
            <td>@item.Price</td>
            <td>@item.Amount</td>
            <td>@item.TeamId</td>
            <td><img src="@item.Picture" style="width:100px;height:150px">
        </tr>
    }

</table>

